# Production Environment for Manual Netlify Deployment
# Copy this content to .env.production before building for deployment

# Backend API URL (update with your actual backend URL)
VITE_API_BASE_URL=https://cdac-rosy.vercel.app/api

# App environment
VITE_APP_ENV=production

# Optional: Analytics (uncomment and configure if needed)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Optional: Error tracking (uncomment and configure if needed)
# VITE_SENTRY_DSN=https://your-sentry-dsn

# Build timestamp (automatically set during build)
VITE_BUILD_TIME=manual-deployment